            <string>if showsettings = "credits" {
    if pressed &gt; 45 then
    showsettings = "settings page"
}
else {
    if pressed &gt; 45 {
        if showsettings = "reset confirmation" {
            //Wipe highscores
            audio_play_sound(nuke_blast,5,false) 
            ini_write_real("highscores","classicscore",0)
            ini_write_real("highscores","coderedscore",0)
            ini_write_real("highscores","crazyscore",0)
            ini_write_real("highscores","classictime",0)
            ini_write_real("highscores","coderedtime",0)
            ini_write_real("highscores","crazytime",0)  
            showsettings = ""
            with (UI_button_codered) {
                locked = true;
                sprite_index = UI_mode_codered_locked;
            }
            with (UI_button_crazy) {
                locked = true;
                sprite_index = UI_mode_crazy_locked;
            } 
        }
        else {
            //Ask for confirmation
            showsettings = "reset confirmation"
            audio_play_sound(spike_sputter,5,false)
        }
    }
    else {
        if showsettings != "reset confirmation" {
            //Invert controls
            ini_write_real("settings","inversion", -1*ini_read_real("settings","inversion",1) )
            showsettings = "invert controls"
        }
        else {
            //Secret unlock stages temporarily
            UI_button_crazy.locked=false
            UI_button_codered.locked=false
            audio_play_sound(award_unlock,5,false) 
            showsettings = "modes unlocked"
        }
    }      
}
pressed = 0
</string>
