        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Script to set player speed

var leftbound = ARENA_LEFTBOUND + +global.roomShift
var rightbound = ARENA_RIGHTBOUND+ +global.roomShift
var xTiltCoord, yTiltCoord;
var mouseWithinBounds;

if YoYo_GetPlatform()==os_windows or YoYo_GetPlatform()==os_macosx {
    mouseWithinBounds = true;
    //find the processed mouse location
    if mouse_x &lt; leftbound { global.mouseX = leftbound; mouseWithinBounds = false; }
    else if mouse_x &gt; rightbound { global.mouseX = rightbound; mouseWithinBounds = false; }
    else { global.mouseX = mouse_x}
    
    if mouse_y &lt; ARENA_TOPBOUND { global.mouseY = ARENA_TOPBOUND; mouseWithinBounds = false; }
    else if mouse_y &gt; ARENA_BOTTOMBOUND { global.mouseY = ARENA_BOTTOMBOUND; mouseWithinBounds = false;}
    else { global.mouseY = mouse_y}
    
    //find speeds
    if burnicadeStage = 2 {
        if speed &lt; PLAYER_BURNICADE_SPEED {
            speed = PLAYER_BURNICADE_SPEED
            direction = point_direction(x,y,global.mouseX,global.mouseY) }
        if angle_difference(point_direction(x,y,global.mouseX,global.mouseY),direction) &lt; 180 and mouseWithinBounds = true {
            move_towards_point(global.mouseX, global.mouseY, speed) } //min(1003,mouse_x)+max(20-mouse_x,0), min(713,mouse_y)+max(55-mouse_y,0)
        else {
            move_towards_point( x + 5*hspeed, y + 5*vspeed, speed) } 
        if y &lt;= ARENA_TOPBOUND or y &gt;= ARENA_BOTTOMBOUND {
            vspeed = 0 }
        if x &lt;= leftbound or x &gt;= rightbound {
            hspeed = 0 } }
    else {
        speed = min(PLAYER_MAXSPEED,(point_distance(x,y,global.mouseX,global.mouseY))/12)
        move_towards_point(global.mouseX, global.mouseY, speed) 
        // Do not vibrate if destination is too close
        if point_distance(global.mouseX,global.mouseY,x,y) &lt; 6 then speed = 0
    }
    hspeed += vortexVectorX
    vspeed += vortexVectorY
}

else
{
    global.htilt=((0.25*power(YoYo_GetTiltY(),3)+0.5*YoYo_GetTiltY())*30)*inversion
    global.vtilt=((0.25*power(YoYo_GetTiltX(),3)+0.5*YoYo_GetTiltX())*30)*inversion
    xTiltCoord = median(leftbound, x+global.htilt, rightbound)
    yTiltCoord = median(ARENA_TOPBOUND, y+global.vtilt, ARENA_BOTTOMBOUND)
    if burnicadeStage = 2 {
        move_towards_point(xTiltCoord, yTiltCoord, sqrt( power(global.htilt,2) + power(global.vtilt,2) ) )  //min(1003,mouse_x)+max(20-mouse_x,0), min(713,mouse_y)+max(55-mouse_y,0)
        if speed != PLAYER_BURNICADE_SPEED {
            speed = PLAYER_BURNICADE_SPEED
            direction = point_direction(x,y,xTiltCoord,yTiltCoord) } }
    else {
        move_towards_point(xTiltCoord, yTiltCoord, sqrt( power(global.htilt,2) + power(global.vtilt,2) ) )
        speed = min(9,speed) }
        
    //hspeed=min( min(1,max(1003-x, 0)) , min(1,max(x-20,0)) ) * htilt
    //vspeed=min( min(1,max(713-x, 0)) , min(1,max(x-55,0)) ) * vtilt
    // Do not vibrate if tilt is too small
    if x &lt; leftbound and global.htilt &lt; 0 then hspeed=0
    if x &gt; rightbound and global.htilt &gt; 0 then hspeed=0
    if y &lt; ARENA_TOPBOUND and global.vtilt &lt; 0 then vspeed=0
    if y &gt; ARENA_BOTTOMBOUND and global.vtilt &gt; 0 then vspeed=0
    
    if abs(global.htilt)&lt;0.2 then hspeed=0
    if abs(global.vtilt)&lt;0.2 then vspeed=0
    
    hspeed += vortexVectorX
    vspeed += vortexVectorY
}
</string>
