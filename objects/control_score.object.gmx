        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw the UI elements

//Draw version number
draw_set_halign(fa_right)
draw_text(view_wview[0]- 20, 742, "Mouseover to Live v" + string(global.version))
draw_set_halign(fa_left)

//Draw score text (sprite)
draw_sprite(spr_score_text,-1,24+global.roomShift,12)

//Draw highscore and best time, if they exist
ini_open("ununoctium.ini")
if !ini_section_exists("highscores") then {ini_write_real("highscores","coderedscore",0); ini_write_real("highscores","classicscore",0); ini_write_real("highscores","crazyscore",0);ini_write_real("highscores","classictime",0);ini_write_real("highscores","coderedtime",0);ini_write_real("highscores","crazytime",0);}
draw_text(800+global.roomShift,15,format_number(abs(highScore)));
draw_text(928+global.roomShift,15,format_time(abs(bestTime)));

//Draw crown - make it gold when player beats requirements for next difficulty
if instance_number(control)&gt;0 {
if score &gt; control_playercolor.unlocknextscore or control.time &gt; control_playercolor.unlocknexttime {
    draw_sprite(crown_gold,-1,770+global.roomShift,24)}
else {
    draw_sprite(crown_silver,-1,770+global.roomShift,24)}
}

//Draw score value (string)
if (highScore &gt; 0) {
    if (room_get_name(room)=="classic" and score &gt; highScore) or (room_get_name(room)=="codered" and score &gt; highScore) or (room_get_name(room)=="crazy" and score &gt; highScore) {
        draw_set_color(c_yellow)
        highScore = -highScore} }
else {draw_set_color(c_yellow)}    
    draw_text(100+global.roomShift,15,format_number(score))
    draw_set_color(c_white)


//Draw current time (string)
if instance_number(control)&gt;0 {
    if (bestTime &gt; 0) {
        if (room_get_name(room)=="classic" and control.time / 60 &gt; bestTime) or (room_get_name(room)=="codered" and control.time / 60 &gt; bestTime) or (room_get_name(room)=="crazy" and control.time / 60 &gt; bestTime) {
            draw_set_color(c_yellow)
            bestTime = -bestTime } }
    else {draw_set_color(c_yellow)}
    draw_text(480+global.roomShift,15,format_time(control.time/60))
    draw_set_color(c_white)
}

//Check if combo &gt; 0
//if global.combo = 0 then return 0
if instance_number(combo_container) = 0 and global.combo &gt;= 5 {
    instance_create(512+global.roomShift,794,combo_container)
}
    //if instance_number(combo_container) = 0 then return 0
    //Draw combo text (sprite)
if instance_number(combo_container) != 0 {
    draw_sprite(spr_combo_text,-1,428+global.roomShift,combo_container.y-18)
    
    //Draw combo value (string)
    draw_set_color(make_color_rgb(56,78,23))
    draw_set_font(font1)
    
    if global.combo &gt;= 5 {
        findCombo = global.combo
    }
    else {
        findCombo = showCombo
    }
    draw_text(518+global.roomShift,combo_container.y-14,"" + string(findCombo*6) + " x " + string(findCombo))
    draw_set_color(c_white)
    draw_set_font(font0)
 }



</string>
