            <kind>1</kind>
            <string>///Draw the combo value after time out
if !instance_exists(player) then return 0
if player.dead = 1 then return 0
if showCombo &lt; 5 then return 0

if player.spikes = 1 or player.lightning = 1 then {
   draw_text(player.x+40, player.y-40, "x"+string(showCombo))
} else if player.shield = 1 then {
  draw_text(player.x+30, player.y-30, "x"+string(showCombo))
} else {
  draw_text(player.x+20, player.y-20, "x"+string(showCombo))
}
</string>
