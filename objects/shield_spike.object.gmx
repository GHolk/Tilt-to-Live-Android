        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Determine angle and size of spike
var playerSpeedRatio, growthSpeed, shrinkSpeed, minSize, maxRotationSpeed, angleDifference;
playerSpeedRatio = player.speed / PLAYER_MAXSPEED;
growthSpeed = 0.03;
shrinkSpeed = 0.03;
minSize = 0.7;
maxRotationSpeed = 7;

angleDifference = player.image_angle - image_angle
if abs(angleDifference) &gt; 180 {
    angleDifference += -360* sign(angleDifference)
}

if abs(angleDifference) &gt; 120 {
    image_angle = player.image_angle
    shrinkSpeed = 0;
}
else if (abs(angleDifference) &gt; maxRotationSpeed) {
    image_angle += sign(angleDifference) * max(maxRotationSpeed, abs(angleDifference)-90)
    shrinkSpeed = 0;
}
else {
    image_angle = player.image_angle
}


image_xscale = clamp(minSize + playerSpeedRatio * (1 - minSize), image_xscale - shrinkSpeed, image_xscale + growthSpeed)
image_yscale = image_xscale
</string>
