            <kind>1</kind>
            <string>///Choose icon identity and initialize settings

if room_get_name(room) == "classic" then image_blend=make_color_rgb(120,180,30)
if room_get_name(room) == "codered" then image_blend=make_color_rgb(244,30,30)
if room_get_name(room) == "crazy" then image_blend=make_color_rgb(150,100,220)

x=9999

do 
sprite_index=choose(UI_background_icon_large,UI_background_icon_medium,UI_background_icon_small)
until sprite_index != instance_nearest(oldx,oldy,spinning_icon).sprite_index

x=oldx

if sprite_index=UI_background_icon_large {rotationspeed=random_range(0.7,0.1);parallax=0.6;image_xscale=-1}
else if sprite_index= UI_background_icon_small {rotationspeed=random_range(0.1,0.7);parallax=1.4}
else {rotationspeed=random_range(-0.7,-0.1);image_xscale=0.7;image_yscale=0.7;parallax=1}

if action_if_dice(2) {
rotationspeed=-rotationspeed
image_xscale=-image_xscale}

image_alpha=0.3
</string>
